{
  "version": "2.0",
  "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[4.0.0, 5.0.0)"
  },
  "eventHubs": {
    "maxEventBatchSize" : 400,
    "maxWaitTime" : "00:05:00",            
    "batchCheckpointFrequency" : 1,
    "prefetchCount" : 800,
    "clientRetryOptions":{
        "mode" : "exponential",
        "tryTimeout" : "00:10:00",
        "delay" : "00:01:00.00",
        "maximumDelay" : "00:10:00",
        "maximumRetries" : 3
    }
  },
  "functions": [
    "EventHubs_Metrics"
  ],
  "functionTimeout": "00:10:00"
}
